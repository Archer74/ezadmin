{
  "name": "ez-admin",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/styled-components": "^4.0.3",
    "apollo-cache-inmemory": "^1.3.9",
    "apollo-client": "^2.4.5",
    "apollo-link": "^1.2.3",
    "apollo-link-context": "^1.0.9",
    "apollo-link-error": "^1.1.1",
    "apollo-link-http": "^1.5.5",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "ramda": "^0.25.0",
    "react": "^16.6.1",
    "react-apollo": "^2.2.4",
    "react-dom": "^16.6.1",
    "react-grid-system": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "^3.1.0",
    "semantic-ui-react": "^0.83.0",
    "styled-components": "^4.1.0"
  },
  "scripts": {
    "dev": "yarn codegen && yarn start",
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "codegen": "yarn get-schema && yarn generate-types",
    "get-schema": "apollo-codegen introspect-schema http://localhost:7930/graphql/ --output \"./src/domain/generated/schema.json\"",
    "generate-types": "apollo-codegen generate \"./src/graphql/**/*.ts\" --schema \"./src/domain/generated/schema.json\" --target typescript --output \"./src/domain/generated/types.ts\" --addTypename"
  },
  "devDependencies": {
    "@types/graphql": "^14.0.3",
    "@types/jest": "^23.3.2",
    "@types/node": "^10.12.5",
    "@types/ramda": "^0.25.41",
    "@types/react": "^16.7.3",
    "@types/react-dom": "^16.0.9",
    "@types/react-router-dom": "^4.3.1",
    "apollo-codegen": "^0.19.0",
    "typescript": "^3.1.6"
  }
}
