{
  "name": "ez-admin",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "apollo-cache-inmemory": "^1.4.2",
    "apollo-client": "^2.4.12",
    "apollo-link": "^1.2.6",
    "apollo-link-context": "^1.0.12",
    "apollo-link-error": "^1.1.5",
    "apollo-link-http": "^1.5.9",
    "formik": "^1.4.2",
    "graphql": "^14.1.1",
    "graphql-tag": "^2.10.1",
    "ramda": "^0.26.1",
    "react": "^16.7.0",
    "react-apollo": "^2.4.0",
    "react-dom": "^16.7.0",
    "react-grid-system": "^4.4.2",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "^3.1.0",
    "semantic-ui-react": "^0.83.0",
    "styled-components": "^4.1.3",
    "yup": "^0.26.7"
  },
  "scripts": {
    "dev": "yarn codegen && yarn start",
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "codegen": "yarn get-schema && yarn generate-types",
    "get-schema": "apollo schema:download --endpoint http://localhost:7930/graphql/ \"./src/domain/generated/schema.json\"",
    "generate-types": "apollo client:codegen --includes=\"./src/graphql/**/*.ts\" --localSchemaFile=\"./src/domain/generated/schema.json\" --target=typescript --addTypename --outputFlat \"./src/domain/generated/types.ts\" "
  },
  "devDependencies": {
    "@types/graphql": "^14.0.5",
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.18",
    "@types/ramda": "^0.25.47",
    "@types/react": "^16.7.20",
    "@types/react-dom": "^16.0.11",
    "@types/react-router-dom": "^4.3.1",
    "@types/styled-components": "^4.1.6",
    "@types/yup": "^0.26.5",
    "typescript": "^3.2.4",
    "apollo": "^2.3.0"
  }
}
